<h1>New Question for <%= @test.title %></h1>

<form action="/tests/<%= @test.id %>/questions" method="post">
  <input name="authenticity_token" value="<%= form_authenticity_token%>" type="hidden">

  <p>
    <label for="question-body">Your question:</label>
    <input type="text" name="question[body]" id="question-body">
  </p><br> 


  <input type="submit" value="Create">
</form>

<br><%=link_to 'Back', test_questions_path %>
